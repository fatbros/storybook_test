{
    "env": {
      "node" : true,
      "browser" : true
    },
    "extends": "airbnb",
    "rules": {
      "no-underscore-dangle": 0,
      "indent": ["error", 4],
      "arrow-parens": 0,
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "react/react-in-jsx-scope": 0,
      "react/jsx-indent": ["error", 4]
    }
}
